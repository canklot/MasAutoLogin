{
    "manifest_version": 3,
    "name": "MAS Auto Login Extension",
    "version": "1.0",
    "description": "Automatically logs in to a specific website every minute.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://172.20.114.21/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}